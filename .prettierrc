{
	"semi": true,
	"singleQuote": false,
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "crlf",
	"plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	]
}
